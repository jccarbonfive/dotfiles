[alias]
    st = status -sb
    br = branch
    ci = commit
    co = checkout
    lol = log --graph
    lola = log --graph --all
    today = log --author='Jared Carroll' --since=midnight
    yesterday = log --author='Jared Carroll' --since=yesterday
[core]
    editor = vi
    excludesfile = ~/.gitignore
[apply]
    whitespace = error
[diff]
    renames = copy
[rerere]
    enabled = true
[merge]
    summary = true
[format]
    pretty = format:%C(yellow)%h%Creset | %C(cyan)%ar%Creset | %s %C(yellow)%d %Cred[%an]
[color]
    ui = auto
[color "status"]
    untracked = yellow
[user]
    name = Jared Carroll Macbook
    email = jared@carbonfive.com
